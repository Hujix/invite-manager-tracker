{
	"compilerOptions": {
		"noImplicitAny": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"target": "es2017",
		"lib": ["es2016", "es2017", "es2018"],
		"sourceMap": true,
		"removeComments": false,
		"experimentalDecorators": true,
		"typeRoots": [],
		"types": ["node"],
		"plugins": [{
			"name": "tslint-language-service"
		}],
		"outDir": "build/"
	}
}
